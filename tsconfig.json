{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "importsNotUsedAsValues": "remove",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@consts/*": ["src/consts/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
    }
  }
}